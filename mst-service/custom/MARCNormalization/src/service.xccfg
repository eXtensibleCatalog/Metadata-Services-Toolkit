NRUDatabaseGenre = 0


# 1 to remove duplicate DCMI type fields. 
DedupDCMIType = 1

# 1 to remove duplicate 007 Vocab fields. 
Dedup007Vocab = 1

# 1 to create a field with a language term for 
# each language code created by the LanguageSplit
