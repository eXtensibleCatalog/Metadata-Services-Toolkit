#####################################################################################
#
# OPTIONAL 014$a processing:
#
# You may filter out 014$a fields via two configuration options, ValidFirstChar014 and/or InvalidFirstChar014
#
#############
#
# ValidFirstChar014 - Remove the 014$a subfield if the first character is *not* one of # the following:
# (Note: the values [none] and [all] have a special meaning.)
#
# Some examples:
#
# E.g., the following means that no 014s are valid (i.e., remove all 014s):
# ValidFirstChar014=[none]
#
# E.g., the following means that all 014s are valid (i.e., retain all 014s):
# ValidFirstChar014=[all]
#
# E.g., the following means that only 014s beginning with a numeric value are valid:
# ValidFirstChar014 = 0123456789
#
#############
#
# InvalidFirstChar014 - Remove the 014$a subfield if the first character *is* one of the # following:
# (Note: the values [none] and [all] have a special meaning.)
#
# Some examples:
#
# E.g., the following means that all 014s are valid (i.e., retain all 014s):
# InvalidFirstChar014=[none]
#
# E.g., the following means that no 014s are valid (i.e., remove all 014s):
# InvalidFirstChar014=[all]
#
# E.g., the following means that all 014s beginning with an alphabetical characters will be removed:
#
# InvalidFirstChar014 = ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz
#
#####################################################################################

# 1 to add a subfield b to the 014 containing the characters
# found in MARCNormalization custom.properties file
# substitutions.014.value, if the field 014 has a value
# beginning with the chars found in the MARCNormalization
# custom.properties file substitutions.014.key
#
# For example by default the file contains:
# substitutions.014.key=ocm
# substitutions.014.value=OCoLC
# so in this case if 014 starts with ocm then add a subfield b
# containing the characters 'OCoLC'
# see custom.properties for more information.
Add014Source = 0

# on - if multiple 004 fields are present, move all but the
# first 004 to valid 014$a fields
# off - leave multiple 004 fields alone as they are (but further
# MST processes, such as marctoxctransformation, will not
# process multiple 004s properly)
# protect - if multiple 004s are present, an error will result
# and the record will not get processed
Replace014 = on

# 1 to replace location codes left by 
# ILS with the actual location. By default 0 
HoldingsLocationName = 0

# 1 to replace location codes by 
# location limit name. By default 0
LocationLimitName = 0

