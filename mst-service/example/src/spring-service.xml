<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
		default-init-method="init">

	<bean name="PropertyPlaceholderConfigurer" class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer" scope="singleton">
		<property name="locations">
			<list>
				<value>classpath:spring.properties</value>
				<value>classpath:spring-custom.properties</value>
			</list>
		</property>
	</bean>
	
	<bean class="xc.mst.spring.MSTBeanPostProcessor"/>
	
	<import resource="spring-service-custom.xml"/>

	<bean id="Service" class="${service.classname}">
	</bean>
		
</beans>